    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.5"
  }
}
